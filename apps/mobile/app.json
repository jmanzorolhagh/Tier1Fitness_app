{
  "expo": {
    "name": "Tier1Fitness",
    "slug": "tier1fitness-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "entryPoint": "./index.js",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jmanzorolhagh.tier1fitnessApp"
    },
    "android": {
      "package": "com.jmanzorolhagh.tier1fitness_app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/fitness.jpg",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-sensors",
        {
          "motionPermission": "Allow $(PRODUCT_NAME) to access your motion data to track steps."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "570ff387-4d79-4816-871d-44915013c90d"
      }
    }
  }
}
